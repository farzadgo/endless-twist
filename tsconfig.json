{
  "compilerOptions": {
    "target": "ESNext",
    "useDefineForClassFields": true,
    "module": "ESNext",
    "lib": ["ESNext", "DOM"],
    "types": ["vite/client", "three", "throttle-debounce"],
    "moduleResolution": "Node",
    "strict": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "noEmit": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "paths": {
      "/@/*": ["./src/*"]
    }
  },
  "include": ["src"]
}

    // "isolatedModules": true,
    // "skipLibCheck": true